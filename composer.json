{
    "name": "adlino/locations",
    "description": "Iran Provinces, Counties and Cities with a Google Map Location for Laravel.",
    "keywords": [
        "Laravel",
        "Locations",
        "Iran",
        "Provinces",
        "Counties",
        "Cities",
        "Latitude",
        "Longitude"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alireza Hamedashki",
            "email": "a.hamedashki@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Adlino\\Locations\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adlino\\Locations\\LocationsServiceProvider"
            ],
            "aliases": {
                "Locations": "Adlino\\Locations\\Facades\\Locations"
            }
        }
    }
}
